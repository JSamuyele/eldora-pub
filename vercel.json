{
  "version": 2,
  "framework": "vite",
  "build": {
    "env": {
      "VITE_API_BASE_URL": "@vite_api_base_url"
    }
  },
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
